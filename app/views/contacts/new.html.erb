<div class = "container-fluid">
  <div class = "row">
    <div class = "js-fullheight" id = "contact-new-left">
    </div>
	<div id = "contact-new-right">
		<div class = "container-fluid" id = "right-header">
		  <p class = "breadcrumbs">
		  	<a href = "#">Home</a>
		  	<a href = "#">Back</a>
		  </p>
		  <h1 class = "bread"> Contact </h1>
		</div>
		<div class = "container" id = "right-head-contents">
	       <div class = "row">
			 <div class = "col-xs-5 contact-new-boxes">
		    	<form action = "#" class = "contact-form">
					<div class="form-group">
						<input type="text" class="form-control" placeholder="Your Name">
					</div>
					<div class="form-group">
						<input type="text" class="form-control" placeholder="Your Email">
					</div>
					<div class="form-group">
						<input type="text" class="form-control" placeholder="Subject">
					</div>
					<div class="form-group">
						<textarea name="" id="" cols="30" rows="7" class="form-control" placeholder="Message"></textarea>
					</div>
					<div class="form-group">
						<input type="submit" value="Send Message" class="btn btn-primary py-3 px-5">
					</div>
				</form>
			</div>
			<div class = "col-xs-5">
		      <div id= "map" class= "bg-light"></div>
		    </div>
		  </div>
  	    </div>
  	</div>
  </div>
</div>
<!-- ユーザーフッター部分テンプレート -->
<%= render 'layouts/users-footer' %>
